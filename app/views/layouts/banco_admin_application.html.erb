<!DOCTYPE html>
<html lang="pt-BR" dir="ltr">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- ==============================================-->
  <!--    Document Title-->
  <!-- ===============================================-->
  <title>Nobe Bank</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag    'banco_admin_application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'banco_admin_application', 'data-turbolinks-track': 'reload' %>
</head>
<body>
<!-- ===============================================-->
<!--    Main Content-->
<!-- ===============================================-->
<main class="main" id="top">
  <div class="container" data-layout="container">
    <nav class="navbar navbar-vertical navbar-expand-xl navbar-light">
      <div class="d-flex align-items-center">
        <div class="toggle-icon-wrapper">
          <button class="btn navbar-toggler-humburger-icon navbar-vertical-toggle" data-toggle="tooltip" data-placement="left" title="Menu de Navegacão"><span class="navbar-toggle-icon"><span class="toggle-line"></span></span></button>
        </div>
        <%= link_to banco_admin_painel_visao_geral_path, class: "navbar-brand" do  %>
          <div class="d-flex align-items-center py-3">
            <%= image_tag("nobebank.png", class: "mr-2", width: 140) %>
          </div>
        <% end %>
      </div>
      <div class="collapse navbar-collapse" id="navbarVerticalCollapse">
        <div class="navbar-vertical-content perfect-scrollbar scrollbar">
          <ul class="navbar-nav flex-column">

          </ul>
          <div class="navbar-vertical-divider">
            <hr class="navbar-vertical-hr my-2" />
          </div>
          <ul class="navbar-nav flex-column">
            <li class="nav-item <%= active_page?(banco_admin_painel_visao_geral_path) %>">
              <%= link_to banco_admin_painel_visao_geral_path, class: "nav-link" do %>
                <div class="d-flex align-items-center">
                  <span class="nav-link-icon"><span class="fas fa-tachometer-alt"></span></span><span class="nav-link-text"> Inicio</span>
                </div>
              <% end %>
            </li>
            <li class="nav-item <%= active_page?(banco_admin_banco_path(Banco.first)) %>">
              <%= link_to banco_admin_banco_path(Banco.first), class: "nav-link" do %>
                <div class="d-flex align-items-center">
                  <span class="nav-link-icon"><span class="fas fa-university"></span></span><span class="nav-link-text"> Banco</span>
                </div>
              <% end %>
            </li>

            <li class="nav-item <%= active_page?(banco_admin_agencia_path) %>">
              <%= link_to banco_admin_agencia_path, class: "nav-link" do %>
                <div class="d-flex align-items-center">
                  <span class="nav-link-icon"><span class="fas fa-money-check-alt"> </span></span><span class="nav-link-text"> Agências</span>
                </div>
              <% end %>
            </li>
            <li class="nav-item <%= active_page?(banco_admin_banco_users_path) %>">
              <%= link_to banco_admin_banco_users_path, class: "nav-link" do %>
                <div class="d-flex align-items-center">
                  <span class="nav-link-icon"><span class="fas fa-user-tie"></span></span><span class="nav-link-text"> Colaborador Administrativo</span>
                </div>
              <% end %>
            </li>
            <li class="nav-item <%= active_page?(banco_admin_cliente_index_path) %>">
              <%= link_to banco_admin_cliente_index_path, class: "nav-link" do %>
                <div class="d-flex align-items-center">
                  <span class="nav-link-icon"><span class="fas fa-user"></span></span><span class="nav-link-text"> Cliente</span>
                </div>
              <% end %>
            </li>
            <div class="settings px-3 px-xl-0">
              <div class="navbar-vertical-divider px-0">
                <hr class="navbar-vertical-hr my-3" />
              </div>
            </div>
            <li class="nav-item">
              <a class="nav-link dropdown-indicator" href="#components" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="components">
                <div class="d-flex align-items-center"><span class="nav-link-icon"><span class="fas fa-cogs"></span></span><span class="nav-link-text">Configurações de Contas</span></div>
              </a>
              <ul class="nav collapse" id="components" data-parent="#navbarVerticalCollapse">
                <li class="nav-item <%= active_page?(banco_admin_conta_tipos_path) %>">
                  <%= link_to banco_admin_conta_tipos_path, class: "nav-link" do %>
                    <div class="d-flex align-items-center">
                      <span class="nav-link-icon"><span class="fas fa-clipboard-list"> </span></span><span class="nav-link-text"> Tipos de Conta</span>
                    </div>
                  <% end %>
                </li>
                <li class="nav-item <%= active_page?(banco_admin_conta_movimento_tipos_path) %>">
                  <%= link_to banco_admin_conta_movimento_tipos_path, class: "nav-link" do %>
                    <div class="d-flex align-items-center">
                      <span class="nav-link-icon"><span class="fas fa-exchange-alt"> </span></span><span class="nav-link-text"> Tipos de Movimento de Conta</span>
                    </div>
                  <% end %>
                </li>
              </ul>
            </li>
            <div class="settings px-3 px-xl-0">
              <div class="navbar-vertical-divider px-0">
                <hr class="navbar-vertical-hr my-3" />
              </div>
            </div>
            <%= link_to 'Sair', banco_admin_auth_logout_path, class:'btn btn-block btn-primary text-white'  %>
          </ul>
        </div>
      </div>
    </nav>
    <div class="content">
      <nav class="navbar navbar-light navbar-glass navbar-top sticky-kit navbar-expand">
        <button class="btn navbar-toggler-humburger-icon navbar-toggler mr-1 mr-sm-3" type="button" data-toggle="collapse" data-target="#navbarVerticalCollapse" aria-controls="navbarVerticalCollapse" aria-expanded="false" aria-label="Toggle Navigation"><span class="navbar-toggle-icon"><span class="toggle-line"></span></span></button>
        <ul class="navbar-nav navbar-nav-icons ml-auto flex-row align-items-center">
          <li class="nav-item">

          </li>
          <li class="nav-item dropdown dropdown-on-hover"><a class="nav-link pr-0" id="navbarDropdownUser" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <div class="avatar avatar-xl">

            </div>
          </a>
            <div class="dropdown-menu dropdown-menu-right py-0" aria-labelledby="navbarDropdownUser">
              <div class="bg-white rounded-soft py-2">
              </div>
            </div>
          </li>
        </ul>
      </nav>
      <% if notice %>
        <div class="alert alert-warning alert-dismissible fade show" role="alert">
          <strong><%= notice %></strong>
          <button class="close" type="button" data-dismiss="alert" aria-label="Close"><span class="font-weight-light" aria-hidden="true">×</span></button>
        </div>
      <% end %>
      <%= yield %>
      <footer>
        <div class="row no-gutters justify-content-between fs--1 mt-4 mb-3">
          <div class="col-12 col-sm-auto text-center">
            <p class="mb-0 text-600">Nobe Bank. <span class="d-none d-sm-inline-block">| </span><br class="d-sm-none" /> 2021 &copy; <a href="http://http://www.nobesistemas.com.br/home" target="_blank">Nobe Bank</a></p>
          </div>
          <div class="col-12 col-sm-auto text-center">
            <p class="mb-0 text-600">v00.1</p>
          </div>
        </div>
      </footer>
    </div>
    <div class="toast bg-white notice" id="cookie-notice" role="alert" data-options='{"autoShow":true,"autoShowDelay":3000,"showOnce":true,"cookieExpireTime":7200000}' data-autohide="false" aria-live="assertive" aria-atomic="true">
      <div class="toast-body d-flex justify-content-center px-5">
        <div class="d-lg-flex align-items-center text-center">
          <button class="close text-shadow-none position-absolute t-0 r-0 p-2 mr-2 mt-2" type="button" data-dismiss="toast" aria-label="Close"><span class="font-weight-medium" aria-hidden="true">×</span></button>
          <p class="mb-2 mb-lg-0">
            Nosso site usa cookies. Ao continuar a usar nosso site, você concorda com nossos <a class="text-underline" href="components/cookie-notice.html">Cookie Politica</a>.</p>
          <button class="btn btn-primary btn-sm ml-3" type="button" data-dismiss="toast" aria-label="Close">Ok, eu entendo!</button>
        </div>
      </div>
    </div>
  </div>
</main>
<!-- ===============================================-->
<!--    End of Main Content-->
<!-- ===============================================-->
<script type='text/javascript'>
    $('.moeda').inputmask( 'currency', {
        "autoUnmask": true,
        radixPoint: ",",
        groupSeparator: ".",
        allowMinus: false,
        digits: 2,
        digitsOptional: false,
        rightAlign: true,
        unmaskAsNumber: true,
        removeMaskOnSubmit: true
    });
    $(".cep").inputmask("99999999");
    $(".telefone").inputmask("(99)9999-9999");
    $(".celular").inputmask("(99)9.9999-9999");
</script>
</body>
</html>